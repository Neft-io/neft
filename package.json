{
  "name": "neft",
  "version": "0.11.23",
  "preferGlobal": true,
  "description": "Universal Platform",
  "keywords": [
    "framework",
    "native",
    "android",
    "ios",
    "webgl"
  ],
  "bin": {
    "neft": "bin/neft.js"
  },
  "main": "index.coffee",
  "license": "Apache-2.0",
  "homepage": "http://neft.io",
  "author": "Krystian Kruk <krystian@neft.io>",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/Neft-io/neft.git"
  },
  "bugs": "https://github.com/Neft-io/neft/issues",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "bundle": "cross-env NODE_PATH=./ coffee --nodejs --harmony scripts/bundle.coffee",
    "test": "node ./bin/neft.js test",
    "update-api-docs": "cross-env NODE_PATH=./ coffee scripts/api-docs.coffee",
    "generate-docs": "coffee ./scripts/docs/",
    "prepublishOnly": "npm run bundle",
    "postinstall": "coffee scripts/ensure-bundle.coffee"
  },
  "dependencies": {
    "acorn": "^5.2.1",
    "babel-core": "6.22.1",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-es2016": "6.22.0",
    "babel-preset-es2017": "6.22.0",
    "chalk": "^2.3.0",
    "chokidar": "1.5.1",
    "coffee-script": "1.10.0",
    "cross-env": "1.0.8",
    "form-data": "1.0.0-rc4",
    "fs-extra": "^4.0.2",
    "glob": "7.0.3",
    "htmlparser2": "3.9.2",
    "http-server": "^0.10.0",
    "image-size": "^0.6.2",
    "jimp": "0.2.27",
    "js-yaml": "3.6.1",
    "mkdirp": "0.5.1",
    "mustache": "2.2.1",
    "node-notifier": "^5.2.1",
    "node-static": "0.7.7",
    "open": "0.0.5",
    "pegjs": "0.10.0",
    "pngjs": "3.0.0",
    "prismjs": "^1.6.0",
    "qs": "6.2.0",
    "semver": "5.1.0",
    "slash": "1.0.0",
    "string-hash": "^1.1.3",
    "uglify-js": "2.7.5",
    "which": "1.2.12",
    "xcode": "0.8.7"
  },
  "devDependencies": {
    "coffee-coverage": "1.0.1",
    "coveralls": "2.11.14",
    "escape-html": "1.0.3",
    "highlight.js": "^9.12.0",
    "imgur": "0.2.1",
    "istanbul": "^0.4.5",
    "less": "^2.7.2",
    "marked": "^0.3.6",
    "wd": "0.4.0"
  },
  "android": {
    "package": "io.neft.tests",
    "versionCode": 1,
    "versionName": "1.0",
    "application": {
      "label": "Neft.io Tests"
    }
  },
  "ios": {
    "package": "io.neft.tests",
    "application": {
      "label": "Neft.io Tests"
    }
  }
}
