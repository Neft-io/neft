@Item body, div, li, pre, layer, article, aside, footer, header, hgroup, main, nav, section, ul, menu, dir, ol, li, dl, dt, dd, form, fieldset, pre {
  layout: 'flow'
  fillWidth: true
}

@Text p, address, blockquote, h1, h2, h3, h4, h5, h6, h7, legend {
  fillWidth: true
}

@Text span, u, strong, b, i, em, code, mark {}

@Image img {}
