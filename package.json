{
  "name": "neft",
  "version": "0.9.13",
  "preferGlobal": true,
  "description": "JavaScript. Everywhere.",
  "keywords": [
    "framework",
    "native",
    "android",
    "ios",
    "webgl"
  ],
  "bin": {
    "neft": "./bin/neft.js"
  },
  "main": "./cli/bundle/neft-node-develop.js",
  "license": "Apache-2.0",
  "homepage": "http://neft.io",
  "author": "Krystian Kruk <krystian@neft.io>",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/Neft-io/neft.git"
  },
  "bugs": "https://github.com/Neft-io/neft/issues",
  "engines": {
    "node": ">=0.11.14"
  },
  "scripts": {
    "bundle": "coffee --nodejs --harmony bundle.coffee",
    "checkout-master": "coffee preversion.coffee; git checkout master; git pull; find ./node_modules/neft-* -type d -maxdepth 0 -execdir sh -c 'echo \\\\n\\\\n$0\\\\n; cd ./node_modules/$0; git checkout master; git pull' {} \\;",
    "test": "node ./node_modules/neft-unit/bin/unit.js ./spec"
  },
  "dependencies": {
    "coffee-script": "^1.10.0",
    "fs-extra": "^0.24.0",
    "htmlparser2": "^3.9.0",
    "js-yaml": "^3.3.1",
    "mkdirp": "^0.5.0",
    "mustache": "^2.2.0",
    "open": "0.0.5",
    "pegjs": "^0.8.0",
    "form-data": "1.0.0-rc3",
    "qs": "^6.0.1",
    "node-static": "^0.7.6",
    "uglify-js": "^2.6.1",
    "semver": "^5.1.0",
    "neft-android-runtime": "Neft-io/neft-android-runtime#4509df0",
    "neft-ios-runtime": "Neft-io/neft-ios-runtime#65dd89f",
    "neft-sample-project": "Neft-io/neft-sample-project#36e6e6d",
    "neft-nml-parser": "Neft-io/neft-nml-parser#5286aad"
  },
  "devDependencies": {
    "glob": "^7.0.3"
  }
}
