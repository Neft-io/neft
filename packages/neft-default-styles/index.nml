const { ImgTag } = require('./index')

@Item body, div, li, pre, layer, article, aside, footer, header, hgroup, main, nav, section, ul, menu, dir, ol, li, dl, dt, dd, form, fieldset, pre {
  layout: 'flow'
  fillWidth: true
}

@Item p, address, blockquote, h1, h2, h3, h4, h5, h6, h7, legend {
  layout: 'flow'
  fillWidth: true

  #text {
    fillWidth: true
  }
}

@Item u, strong, b, i, em, code, mark {
  layout: 'flow'

  #text {
    fillWidth: true
  }
}

@Image img {}
